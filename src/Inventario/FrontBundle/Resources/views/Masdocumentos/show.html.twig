{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block body -%}

   <div id="sitecontent"> 
        <div id="menubar"> 
            <ul id="menu">
              <!-- put class="selected" in the li tag for the selected page - to highlight which page you're on -->
              <li class="selected"><a href="{{ path('terceros') }}">Lista de terceros</a></li>
              <li><a href="{{ path('terceros') }}">Crear tercero</a></li>
            </ul>
        </div>    
    </div>    
    <h1>Masdocumentos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Txnumdoc</th>
                <td>{{ entity.txnumdoc }}</td>
            </tr>
            <tr>
                <th>Fefecha</th>
                <td>{{ entity.fefecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fevencimiento</th>
                <td>{{ entity.fevencimiento|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Txobservaciones</th>
                <td>{{ entity.txobservaciones }}</td>
            </tr>
            <tr>
                <th>Txcondpago</th>
                <td>{{ entity.txcondpago }}</td>
            </tr>
            <tr>
                <th>Dbvalneto</th>
                <td>{{ entity.dbvalneto }}</td>
            </tr>
            <tr>
                <th>Dbvaliva</th>
                <td>{{ entity.dbvaliva }}</td>
            </tr>
            <tr>
                <th>Dbtotal</th>
                <td>{{ entity.dbtotal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('masdocumentos') }}">
            Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('masdocumentos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
