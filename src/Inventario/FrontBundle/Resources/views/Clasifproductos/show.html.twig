{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block sitecontent -%}

    <h1>Clasifproductos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Txdescripcion</th>
                <td>{{ entity.txdescripcion }}</td>
            </tr>
            <tr>
                <th>Intipo</th>
                <td>{{ entity.intipo }}</td>
            </tr>
            <tr>
                <th>Clase principal</th>
                <td>{{ entity.inpadre }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('clasifproductos') }}">
            Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('clasifproductos_edit', { 'id': entity.id }) }}">
            Modificar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
