{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Detdocumentos list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Incantidad</th>
                <th>Dbvalunitario</th>
                <th>Dbvaltotal</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('detdocumentos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.incantidad }}</td>
                <td>{{ entity.dbvalunitario }}</td>
                <td>{{ entity.dbvaltotal }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('detdocumentos_show', { 'id': entity.id }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('detdocumentos_edit', { 'id': entity.id }) }}">Modificar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('detdocumentos_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
