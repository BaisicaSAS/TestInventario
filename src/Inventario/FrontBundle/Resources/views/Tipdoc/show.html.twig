{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block body -%}

    <h1>Tipos de documento</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Txtipdoc</th>
                <td>{{ entity.txtipdoc }}</td>
            </tr>
            <tr>
                <th>Txnomdoc</th>
                <td>{{ entity.txnomdoc }}</td>
            </tr>
            <tr>
                <th>Inafecta</th>
                <td>{{ entity.inafecta }}</td>
            </tr>
            <tr>
                <th>Intipter</th>
                <td>{{ entity.intipter }}</td>
            </tr>
            <tr>
                <th>Txobservplantilla</th>
                <td>{{ entity.txobservplantilla }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tipdoc') }}">
            Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('tipdoc_edit', { 'id': entity.id }) }}">
            Modificar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
