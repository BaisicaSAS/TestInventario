{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block body -%}

    <h1>Tipos de documento</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Txtipdoc</th>
                <th>Txnomdoc</th>
                <th>Inafecta</th>
                <th>Intipter</th>
                <th>Txobservplantilla</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('tipdoc_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.txtipdoc }}</td>
                <td>{{ entity.txnomdoc }}</td>
                <td>{{ entity.inafecta }}</td>
                <td>{{ entity.intipter }}</td>
                <td>{{ entity.txobservplantilla }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('tipdoc_show', { 'id': entity.id }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('tipdoc_edit', { 'id': entity.id }) }}">Modificar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('tipdoc_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
