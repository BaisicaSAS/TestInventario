{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block body -%}

   <div id="sitecontent"> 
        <div id="menubar"> 
            <ul id="menu">
              <!-- put class="selected" in the li tag for the selected page - to highlight which page you're on -->
              <li class="selected"><a href="{{ path('terceros') }}">Lista de terceros</a></li>
              <li><a href="{{ path('terceros') }}">Crear tercero</a></li>
            </ul>
        </div>    
    </div>    

    <h1>Tipdoc list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Txtipdoc</th>
                <th>Txnomdoc</th>
                <th>Inafecta</th>
                <th>Intipter</th>
                <th>Txobservplantilla</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('tipdoc_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.txtipdoc }}</td>
                <td>{{ entity.txnomdoc }}</td>
                <td>{{ entity.inafecta }}</td>
                <td>{{ entity.intipter }}</td>
                <td>{{ entity.txobservplantilla }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('tipdoc_show', { 'id': entity.id }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('tipdoc_edit', { 'id': entity.id }) }}">Modificar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('tipdoc_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
