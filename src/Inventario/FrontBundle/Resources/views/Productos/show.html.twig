{% extends "InventarioFrontBundle:Default:index.html.twig" %}

{% block sitecontent -%}

    <h1>Productos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Txrefinterna</th>
                <td>{{ entity.txrefinterna }}</td>
            </tr>
            <tr>
                <th>Txrefexterna</th>
                <td>{{ entity.txrefexterna }}</td>
            </tr>
            <tr>
                <th>Txnomproducto</th>
                <td>{{ entity.txnomproducto }}</td>
            </tr>
            <tr>
                <th>Txdescripcion</th>
                <td>{{ entity.txdescripcion }}</td>
            </tr>
            <tr>
                <th>Inactivo</th>
                <td>{{ entity.inactivo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('productos') }}">
            Volver al listado
        </a>
    </li>
    <li>
        <a href="{{ path('productos_edit', { 'id': entity.id }) }}">
            Modificar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
